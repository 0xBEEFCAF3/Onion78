<?xml version="1.0" encoding="UTF-8"?>
<rxui:ReactiveContentPage x:Class="Chaincase.Views.CoinDetailModal"
                          x:TypeArguments="vm:CoinViewModel"
                          xmlns:vm="clr-namespace:Chaincase.ViewModels;assembly=Chaincase"
                          xmlns:rxui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
                          xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                          xmlns="http://xamarin.com/schemas/2014/forms"
                          xmlns:controls="clr-namespace:Chaincase.Controls"
                          Title="Coin Details">
  <StackLayout>
    <controls:NeuButton x:Name="BackButton"
                        Text="BACK" />
    <Label x:Name="AmountBtc"/>

    <Label Text="Transaction ID:"/>
    <Label x:Name="TransactionId"/>

    <Label x:Name="OutputIndex"/>

    <Label x:Name="Confirmations"/>

    <Label x:Name="AnonymitySet"/>

    <Label x:Name="Clusters"/>
  </StackLayout>
</rxui:ReactiveContentPage>
