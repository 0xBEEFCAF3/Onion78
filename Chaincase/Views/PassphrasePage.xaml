<?xml version="1.0" encoding="utf-8"?>
<rxui:ReactiveContentPage x:Class="Chaincase.Views.PassphrasePage"
                          x:TypeArguments="vm:PassphraseViewModel"
                          xmlns:vm="clr-namespace:Chaincase.ViewModels;assembly=Chaincase"
                          xmlns:rxui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
                          xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                          xmlns="http://xamarin.com/schemas/2014/forms">
  <StackLayout Margin="10,35,10,10">
    <Label Text="Passphrase"
           HorizontalOptions="Center"
           FontAttributes="Bold" />
    <Entry x:Name="Passphrase"
           Placeholder="thIsisAPaSsPhrAseToGenerateKeys"
           IsPassword="true" />
    <Button x:Name="Submit"
            Text="Submit" />
  </StackLayout>
</rxui:ReactiveContentPage>