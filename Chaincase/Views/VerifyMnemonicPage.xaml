<?xml version="1.0" encoding="UTF-8"?>
<rxui:ReactiveContentPage x:Class="Chaincase.Views.VerifyMnemonicPage"
                          x:TypeArguments="vm:VerifyMnemonicViewModel"
                          xmlns:vm="clr-namespace:Chaincase.ViewModels;assembly=Chaincase"
                          xmlns:rxui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
                          xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                          xmlns="http://xamarin.com/schemas/2014/forms"
                          xmlns:controls="clr-namespace:Chaincase.Controls"
                          Title="Verify Words">
  <StackLayout x:Name="Stack">
    <Label x:Name="InstructionLabel" Text="Pick word 1"/>

    <controls:NeuButton x:Name="ButtonA" />
    <controls:NeuButton x:Name="ButtonB" />
    <controls:NeuButton x:Name="ButtonC" />
    <controls:NeuButton x:Name="ButtonD" />
    <controls:NeuButton x:Name="ButtonE" />
  </StackLayout>
</rxui:ReactiveContentPage>
