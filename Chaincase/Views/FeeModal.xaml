<?xml version="1.0" encoding="utf-8"?>
<rxui:ReactiveContentPage x:Class="Chaincase.Views.FeeModal"
                          x:TypeArguments="vm:FeeViewModel"
                          xmlns:vm="clr-namespace:Chaincase.ViewModels;assembly=Chaincase"
                          xmlns:rxui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
                          xmlns:templates="clr-namespace:Chaincase.Views.Templates;assembly=Chaincase"
                          xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                          xmlns="http://xamarin.com/schemas/2014/forms">
  <StackLayout VerticalOptions="Center">
    <Label x:Name="FeeLabel" HorizontalTextAlignment="Center"/>
    <Label x:Name="FeeTargetTimeLabel"  HorizontalTextAlignment="Center"/>
    <Slider x:Name="FeeSlider" />
  </StackLayout>
</rxui:ReactiveContentPage>

